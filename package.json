{
  "name": "openapicmd",
  "description": "OpenAPI Command Line Tool",
  "version": "1.13.1",
  "author": "Viljami Kuosmanen <viljami@viljami.io>",
  "bin": {
    "openapi": "bin/run"
  },
  "bugs": "https://github.com/anttiviljami/openapicmd/issues",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@koa/cors": "^3.0.0",
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.12.9",
    "@oclif/plugin-help": "^2.1.6",
    "@types/inquirer": "^7.3.1",
    "axios": "^0.21.1",
    "chalk": "^4.0.0",
    "cli-ux": "^5.4.5",
    "debug": "^4.1.1",
    "deepmerge": "^4.2.2",
    "get-port": "^5.0.0",
    "inquirer": "^7.1.0",
    "js-yaml": "^3.13.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-mount": "^4.0.0",
    "koa-proxy": "^1.0.0-alpha.0",
    "koa-router": "^9.4.0",
    "koa-static": "^5.0.0",
    "openapi-backend": "^5.1.0",
    "openapi-client-axios": "^5.1.0",
    "swagger-editor-dist": "^4.0.4",
    "swagger-ui-dist": "^4.1.3",
    "swagger2openapi": "^7.0.2",
    "tslib": "^2.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.9",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/debug": "^4.1.5",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.40",
    "@types/koa-static": "^4.0.1",
    "@types/node": "^13.11.1",
    "@types/swagger-ui-dist": "^3.30.1",
    "@types/yamljs": "^0.2.30",
    "chai": "^4.2.0",
    "globby": "^11.0.0",
    "jest": "^27.4.4",
    "nock": "^12.0.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.3",
    "wait-on": "^4.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/anttiviljami/openapicmd",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "openapi",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "anttiviljami/openapicmd",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && npm run build && oclif-dev manifest && npm run readme",
    "readme": "bin/run readme",
    "watch-build": "tsc -w",
    "build": "tsc -b",
    "test": "jest -i",
    "lint": "tslint -p . -t stylish",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
