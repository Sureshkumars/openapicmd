{
  "name": "openapicmd",
  "description": "OpenAPI Command Line Tool",
  "version": "2.0.0-rc1",
  "author": "Viljami Kuosmanen <viljami@viljami.io>",
  "bin": {
    "openapi": "./bin/run.js"
  },
  "bugs": "https://github.com/openapistack/openapicmd/issues",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@koa/cors": "^4.0.0",
    "@oclif/config": "^1.18.17",
    "@oclif/core": "^3",
    "@oclif/errors": "^1.3.6",
    "@oclif/plugin-plugins": "^3.9.1",
    "@types/inquirer": "^7.3.1",
    "ajv": "^8.12.0",
    "axios": "^1.3.4",
    "chalk": "^4.0.0",
    "cli-ux": "^6.0.9",
    "debug": "^4.1.1",
    "deepmerge": "^4.3.0",
    "get-port": "^5.0.0",
    "inquirer": "^7.1.0",
    "js-yaml": "^4.1.0",
    "klona": "^2.0.6",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-mount": "^4.0.0",
    "koa-proxy": "^1.0.0-alpha.3",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "openapi-backend": "^5.6.2",
    "openapi-client-axios": "^7.1.1",
    "swagger-editor-dist": "^4.6.1",
    "swagger-ui-dist": "^4.16.0",
    "swagger2openapi": "^7.0.8",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@oclif/command": "^1.8.36",
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^3",
    "@oclif/tslint": "^3.1.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.5.1",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-logger": "^3.1.2",
    "@types/node": "^18.14.1",
    "@types/swagger-ui-dist": "^3.30.1",
    "chai": "^4.2.0",
    "globby": "^11.0.0",
    "jest": "^29.7.0",
    "nock": "^13.3.0",
    "oclif": "^4.0.2",
    "openapi-client-axios-typegen": "*",
    "openapi-types": "^12.1.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5",
    "wait-on": "^4.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://openapistack.co",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "openapi",
    "topicSeparator": " ",
    "topics": {
      "test": {
        "description": "Run automated tests against APIs"
      }
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "openapistack/openapicmd",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && npm run build && oclif manifest && oclif readme",
    "readme": "npm run build && oclif readme",
    "watch-build": "tsc -w",
    "build": "tsc -b",
    "test": "jest -i",
    "lint": "tslint -p . -t stylish",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}